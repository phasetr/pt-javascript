{
	"name": "console",
	"version": "1.0.0",
	"description": "Data seeding console application",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"console-seed": "dist/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/cli.ts",
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"clean:build": "rm -rf dist tsconfig.tsbuildinfo",
		"lint": "biome check .",
		"lint:fix": "biome check --fix .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:dev": "vitest run --maxWorkers=1",
		"test:watch": "vitest",
		"seed": "tsx src/cli.ts"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "3.848.0",
		"@aws-sdk/lib-dynamodb": "3.848.0",
		"commander": "14.0.0",
		"ulid": "2.3.0"
	},
	"devDependencies": {
		"@types/node": "24.1.0",
		"tsx": "4.20.3",
		"typescript": "5.8.3",
		"vitest": "3.2.4"
	}
}
