{
	"name": "benchmark",
	"version": "1.0.0",
	"description": "Benchmark execution application",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"benchmark-run": "dist/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/cli.ts",
		"clean": "rm -rf dist",
		"clean:build": "rm -rf dist",
		"lint": "biome check .",
		"lint:fix": "biome check --fix .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:dev": "vitest run --maxWorkers=1",
		"test:watch": "vitest",
		"run": "tsx src/cli.ts"
	},
	"dependencies": {
		"@aws-sdk/client-cloudformation": "^3.654.0",
		"commander": "^12.1.0",
		"ulid": "^2.3.0"
	},
	"devDependencies": {
		"@types/node": "^22.7.9",
		"tsx": "^4.19.1",
		"typescript": "^5.5.4",
		"vitest": "^2.1.2"
	}
}
