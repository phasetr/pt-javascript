{
	"compilerOptions": {
		"outDir": "build",
		"target": "ESNext",
		"module": "NodeNext",
		"strict": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"esModuleInterop": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
